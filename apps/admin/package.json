{
  "name": "admin",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "deploy": "vercel deploy dist --team=turborepo --confirm",
    "dev": "concurrently \"npm:dev:*\"",
    "dev:vite": "vite --host 0.0.0.0 --port 3001 --clearScreen false",
    "dev:relay": "relay-compiler --watch",
    "relay": "relay-compiler",
    "build": "relay-compiler && tsc --noEmit && vite build",
    "lint": "tsc --noEmit && TIMING=1 eslint src/**/*.ts* --fix"
  },
  "dependencies": {
    "@tanstack/react-location": "^3.7.4",
    "@tanstack/react-location-devtools": "^3.4.4",
    "graphql": "16.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-relay": "^14.1.0",
    "react-error-boundary": "^3.1.4",
    "relay-runtime": "^14.1.0",
    "recoil": "^0.7.5",
    "ui": "*"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.7",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@types/react-relay": "^14.1.1",
    "@vitejs/plugin-react": "^2.1.0",
    "autoprefixer": "^10.4.8",
    "babel-plugin-relay": "^14.1.0",
    "concurrently": "^7.4.0",
    "eslint": "^8.23.0",
    "eslint-config-custom": "*",
    "postcss": "^8.4.16",
    "tsconfig": "*",
    "typescript": "^4.8.3",
    "relay-compiler": "^14.1.0",
    "relay-compiler-language-typescript": "^15.0.1",
    "relay-test-utils": "14.1.0",
    "tailwindcss": "^3.1.8",
    "vite": "^3.1.0",
    "vite-plugin-relay": "^2.0.0"
  }
}
